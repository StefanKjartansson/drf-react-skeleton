{
  "presets": [
    "es2015",
    "react"
  ],
  "plugins": [
    "transform-class-properties"
  ],
  "env": {
    "development": {
      "plugins": [
        "import-asserts"
      ]
    },
    "production": {
      "presets": [
        "es2015",
        "react",
        "react-optimize"
      ]
    }
  }
}
